{
  "name": "whitelistnft",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/hardhat",
      "packages/nextjs"
    ]
  },
  "scripts": {
    "account": "yarn workspace @whitelistnft/hardhat account",
    "chain": "yarn workspace @whitelistnft/hardhat chain",
    "fork": "yarn workspace @whitelistnft/hardhat fork",
    "deploy": "yarn workspace @whitelistnft/hardhat deploy",
    "verify": "yarn workspace @whitelistnft/hardhat verify",
    "hardhat-verify": "yarn workspace @whitelistnft/hardhat hardhat-verify",
    "compile": "yarn workspace @whitelistnft/hardhat compile",
    "generate": "yarn workspace @whitelistnft/hardhat generate",
    "flatten": "yarn workspace @whitelistnft/hardhat flatten",
    "hardhat:lint": "yarn workspace @whitelistnft/hardhat lint",
    "hardhat:lint-staged": "yarn workspace @whitelistnft/hardhat lint-staged",
    "hardhat:format": "yarn workspace @whitelistnft/hardhat format",
    "hardhat:test": "yarn workspace @whitelistnft/hardhat test",
    "test": "yarn hardhat:test",
    "format": "yarn next:format && yarn hardhat:format",
    "start": "yarn workspace @whitelistnft/nextjs dev",
    "next:lint": "yarn workspace @whitelistnft/nextjs lint",
    "next:format": "yarn workspace @whitelistnft/nextjs format",
    "next:check-types": "yarn workspace @whitelistnft/nextjs check-types",
    "next:build": "yarn workspace @whitelistnft/nextjs build",
    "postinstall": "husky install",
    "precommit": "lint-staged",
    "vercel": "vercel",
    "vercel:yolo": "vercel --build-env NEXT_PUBLIC_IGNORE_BUILD_ERROR=true"
  },
  "packageManager": "yarn@3.2.3",
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "next": "^14.0.4",
    "vercel": "^32.4.1"
  }
}
